{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run app.main",
      "type": "python",
      "request": "launch",
      "module": "app.main",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONDONTWRITEBYTECODE": "1"
      }
    },
    {
      "name": "JupyterLab (port 8888, no-token)",
      "type": "python",
      "request": "launch",
      "module": "jupyter",
      "args": [
        "lab",
        "--ip=0.0.0.0",
        "--port=8888",
        "--no-browser",
        "--NotebookApp.token=",
        "--NotebookApp.password=",
        "--allow-root"
      ],
      "console": "integratedTerminal"
    }
  ]
}
